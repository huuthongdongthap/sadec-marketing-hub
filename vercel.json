{
    "version": 2,
    "buildCommand": null,
    "outputDirectory": ".",
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=0, must-revalidate"
                }
            ]
        }
    ],
    "rewrites": [
        {
            "source": "/admin-dashboard.html",
            "destination": "/admin/dashboard.html"
        },
        {
            "source": "/portal-dashboard.html",
            "destination": "/portal/dashboard.html"
        },
        {
            "source": "/portal/payments",
            "destination": "/portal/payments.html"
        },
        {
            "source": "/portal/payment-result",
            "destination": "/portal/payment-result.html"
        },
        {
            "source": "/portal/approve",
            "destination": "/portal/approve.html"
        },
        {
            "source": "/portal/reports",
            "destination": "/portal/reports.html"
        },
        {
            "source": "/portal/assets",
            "destination": "/portal/assets.html"
        }
    ]
}